{
  "expo": {
    "name": "Time Tamer",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#eab800"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon",
        "backgroundColor": "#FFFFFF"
      },
      "package": "dev.kurtw.tasks"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
